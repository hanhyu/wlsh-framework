{
  "name": "wlsh/frame",
  "description": "运行环境：php 7.2.5 / nginx 1.14.0 / swoole 4.2.5 / yaf 3.0.7 / Mysql 8.0.11/ Redis 4.0.10 / inotify",
  "keywords": [
    "swoole",
    "redis",
    "php",
    "yaf",
    "medoo"
  ],
  "authors": [
    {
      "name": "hanhyu",
      "email": "hanhyu@qq.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=7.2",
    "catfan/medoo": "1.7.6",
    "monolog/monolog": "2.0.0",
    "swiftmailer/swiftmailer": "6.2.1",
    "webgeeker/validation": "0.4.4",
    "mongodb/mongodb": "1.5.1"
  },
  "repositories": {
    "packagist": {
      "type": "composer",
      "url": "https://mirrors.aliyun.com/composer/"
    }
  },
  "autoload": {
    "psr-4": {
      "App\\Models\\": "application/models",
      "App\\Domain\\": "application/domain",
      "App\\Plugins\\": "application/plugins",
      "App\\Modules\\": "application/modules",
      "Tests\\": "tests/"
    }
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^2.14@dev",
    "phpunit/phpunit": "^7.4",
    "elasticsearch/elasticsearch": "~6.0",
    "hightman/xunsearch": "1.4.13",
    "swoole/ide-helper": "4.4.10"
  }
}
