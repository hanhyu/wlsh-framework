{
  "name": "wlsh/frame",
  "description": "运行环境：php 8.0 / nginx 1.18.0 / swoole 4.5.9 / Mysql 8.0 / Redis 6.0 / inotify",
  "keywords": [
    "swoole",
    "php"
  ],
  "authors": [
    {
      "name": "hanhyu",
      "email": "hanhyu@qq.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=8.0",
    "ext-swoole": ">=4.6.2",
    "ext-mongodb": ">=1.9.0",
    "ext-redis": ">=5.3.2",
    "monolog/monolog": "2.2.0",
    "swiftmailer/swiftmailer": "6.2.5",
    "webgeeker/validation": "0.5.2",
    "mongodb/mongodb": "1.8.0",
    "elasticsearch/elasticsearch": "^7.11.x-dev",
    "longlang/phpkafka": "1.1.1",
    "envms/fluentpdo": "2.2.0"
  },
  "repositories": {
    "packagist": {
      "type": "composer",
      "url": "https://mirrors.aliyun.com/composer/"
    }
  },
  "autoload": {
    "psr-4": {
      "App\\Models\\": "application/models",
      "App\\Domain\\": "application/domain",
      "App\\Modules\\": "application/modules",
      "App\\Library\\": "application/library",
      "App\\Controllers\\": "application/controllers",
      "App\\Services\\": "application/services",
      "Tests\\": "tests/",
      "Console\\Controllers\\": "console/controllers",
      "Console\\Models\\": "console/models"
    }
  },
  "require-dev": {
    "roave/security-advisories": "dev-master",
    "friendsofphp/php-cs-fixer": "^3.0.x-dev",
    "phpunit/phpunit": "^9.5.0",
    "hightman/xunsearch": "1.4.13",
    "swoole/ide-helper": "4.6.2"
  }
}
