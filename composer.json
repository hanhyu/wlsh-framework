{
  "name": "wlsh/frame",
  "description": "运行环境：php 8.0 / nginx 1.18.0 / swoole 4.5.9 / Mysql 8.0 / Redis 6.0 / inotify",
  "keywords": [
    "swoole",
    "php"
  ],
  "authors": [
    {
      "name": "hanhyu",
      "email": "hanhyu@qq.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=8.0",
    "catfan/medoo": "1.7.6",
    "monolog/monolog": "2.0.0",
    "swiftmailer/swiftmailer": "6.2.1",
    "webgeeker/validation": "0.4.4",
    "mongodb/mongodb": "1.5.1",
    "elasticsearch/elasticsearch": "^7.9",
    "longlang/phpkafka": "^1.0"
  },
  "repositories": {
    "packagist": {
      "type": "composer",
      "url": "https://mirrors.aliyun.com/composer/"
    }
  },
  "autoload": {
    "psr-4": {
      "App\\Models\\": "application/models",
      "App\\Domain\\": "application/domain",
      "App\\Modules\\": "application/modules",
      "App\\Library\\": "application/library",
      "App\\Controllers\\": "application/controllers",
      "Tests\\": "tests/"
    }
  },
  "require-dev": {
    "roave/security-advisories": "dev-master",
    "friendsofphp/php-cs-fixer": "^2.14@dev",
    "phpunit/phpunit": "^7.4",
    "hightman/xunsearch": "1.4.13",
    "swoole/ide-helper": "4.4.10"
  }
}
