{
  "name": "wlsh/frame",
  "description": "运行环境：php 7.2.5 / nginx 1.14.0 / swoole 4.2.5 / yaf 3.0.7 / Mysql 8.0.11/ Redis 4.0.10 / inotify",
  "keywords": [
    "swoole",
    "redis",
    "php",
    "yaf",
    "medoo"
  ],
  "authors": [
    {
      "name": "hanhyu",
      "email": "hanhyu@qq.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=7.2",
    "catfan/Medoo": "1.6.1",
    "monolog/monolog": "2.0.0-beta1",
    "swiftmailer/swiftmailer": "6.1.3",
    "webgeeker/validation": "^0.4"
  },
  "repositories": {
    "packagist": {
      "type": "composer",
      "url": "https://packagist.phpcomposer.com"
    }
  },
  "autoload": {
    "psr-4": {
      "App\\Models\\": "application/models",
      "App\\Domain\\": "application/domain",
      "App\\Library\\": "application/library",
      "App\\Plugins\\": "application/plugins",
      "Tests\\": "tests/"
    }
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^2.14@dev",
    "phpunit/phpunit": "^7.4",
    "elasticsearch/elasticsearch": "~6.0",
    "hightman/xunsearch": "1.4.13",
    "mongodb/mongodb": "1.4.2"
  }
}
